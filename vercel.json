{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "regions": ["fra1"],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "master": false
    }
  },
  "github": {
    "enabled": true,
    "silent": true
  },
  "env": {
    "NEXT_PUBLIC_API_URL": "@next_public_api_url",
    "NODE_ENV": "production"
  }
}
